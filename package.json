{
  "name": "awwwwards",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "./node_modules/node-sass/bin/node-sass --watch front/sass/main.scss public/src/css/app.css && webpack --watch --config webpack.config.js",
    "dev": "./node_modules/node-sass/bin/node-sass front/sass/main.scss public/src/css/app.css && node ./node_modules/postcss/lib/postcss.js --use autoprefixer --autoprefixer.browsers '> 0%' public/src/css/app.css -d public/src/css/ && webpack --config webpack.config.js",
    "prod": "node ./node_modules/minifier/index.js public/src/css/app.css && webpack -p --config webpack.config.js"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "minifier": "^0.8.0",
    "node-sass": "^3.8.0",
    "postcss": "^5.1.2",
    "webpack": "^1.13.1",
    "eslint": "^3.7.1",
    "sass-lint": "^1.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nansthomas/awwwwards.git"
  },
